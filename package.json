{
  "name": "vivy",
  "productName": "VIVY",
  "appId": "io.liriliri.vivy",
  "version": "1.0.3",
  "description": "Stable diffusion desktop application",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:preload\" \"npm run dev:renderer\"",
    "dev:main": "vite build --watch --config vite.main.ts --mode=development",
    "dev:preload": "vite build --watch --config vite.preload.ts --mode=development",
    "dev:renderer": "vite",
    "test": "vitest",
    "build": "zx script/build.mjs",
    "pack": "zx script/pack.mjs",
    "suggestions": "zx script/suggestions.mjs",
    "build:main": "vite build --config vite.main.ts",
    "build:preload": "vite build --config vite.preload.ts",
    "build:renderer": "vite build",
    "start": "electron ./dist/main/index.js --inspect=9229",
    "format": "lsla prettier \"webui/env/*.mjs\" \"src/**/*.{ts,tsx,scss,css,json}\" \"test/*.test.ts\" \"*.{ts,json}\" \"script/*.mjs\" --write",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "genIcon": "lsla genIcon --input src/renderer/ --output src/renderer/icon.css --name vivy-icon --source src/renderer/icon/",
    "genTheme": "lsla genTheme --input src/common/theme.json --output src/renderer/theme.scss && lsla genTheme --input src/common/theme.json --output src/common/theme.ts",
    "genPb": "cd src/renderer/main/lib && pbjs -t static-module -w es6 -o vivyFile.js vivyFile.proto && pbts -o vivyFile.d.ts vivyFile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/vivy.git"
  },
  "author": "surunzi",
  "bugs": {
    "url": "https://github.com/liriliri/vivy/issues"
  },
  "homepage": "https://github.com/liriliri/vivy#readme",
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@monaco-editor/react": "^4.5.2",
    "@types/exif": "^0.6.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.0.0",
    "@xterm/addon-canvas": "^0.7.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.7.2",
    "concurrently": "^8.0.1",
    "custom-electron-titlebar": "^4.2.7",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "electron-updater": "^6.6.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "execa": "^7.1.1",
    "exif": "^0.6.0",
    "exifr": "^7.1.3",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "install": "^0.13.0",
    "jsdom": "^24.1.0",
    "licia": "^1.47.0",
    "luna-cropper": "^0.2.1",
    "luna-data-grid": "^1.6.5",
    "luna-image-viewer": "^1.1.0",
    "luna-mask-editor": "^0.5.1",
    "luna-menu": "^0.1.2",
    "luna-modal": "^1.3.3",
    "luna-notification": "^0.3.3",
    "luna-object-viewer": "^0.3.1",
    "luna-painter": "^0.5.0",
    "luna-performance-monitor": "^1.2.2",
    "luna-setting": "^2.0.2",
    "luna-split-pane": "^0.4.1",
    "luna-tab": "^0.4.3",
    "luna-toolbar": "^0.9.2",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^4.0.2",
    "monaco-editor": "^0.25.2",
    "npm": "^10.5.0",
    "pidusage": "^3.0.2",
    "png-chunks-extract": "^1.0.0",
    "protobufjs-cli": "^1.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.85.1",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.32.0",
    "vite": "^5.4.9",
    "vitest": "^1.6.0",
    "zx": "^7.2.2"
  },
  "dependencies": {
    "@parcel/watcher": "^2.4.1",
    "protobufjs": "^7.2.6"
  }
}
